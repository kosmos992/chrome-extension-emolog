{
  "short_name": "omu",
  "name": "omu",
  "icons": { "16": "omu2.ico" },
  "description": "Chrome homepage extension, in which you can record your daily moods and feelings.",
  "version": "0.0.0.1",
  "manifest_version": 3,
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "action": {
    "default_icon": { "16": "omu2.ico" }
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  },
  "permissions": ["storage", "unlimitedStorage"]
}
